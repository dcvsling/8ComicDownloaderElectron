{
  "name": "8comic-downloader-electron",
  "description": "8comic(comicbus.com) downloader",
  "version": "1.5.1",
  "main": "electron-main.js",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "cp ./package.json ./src/package.json && ng test --single-run",
    "test_cont": "cp ./package.json ./src/package.json && ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "clean": "rm -rf ./dist && rm -rf ./release",
    "prepare_build": "cp ./package.json ./src/package.json && cp ./node_modules/jquery/dist/jquery.min.js ./src/assets && cp ./node_modules/bootstrap/dist/js/bootstrap.min.js ./src/assets && ng build",
    "start": "npm run prepare_build && concurrently --raw \"electron ./dist\" \"ng build -w\"",
    "build_mac": "npm run prepare_build && electron-packager ./dist --platform=darwin --arch=x64 --out=./release --icon=src/assets/icons/icon.icns --asar=true --overwrite --app-bundle-id=\"tw.idv.wellwind.8comicdownloaderelectron\" --version=1.4.0",
    "build_win32": "npm run prepare_build && electron-packager ./dist --platform=win32 --arch=x64 --out=./release --icon=src/assets/icons/icon.png --asar=true --overwrite --version=1.4.0",
    "build_linux": "npm run prepare_build && electron-packager ./dist --platform=linux --arch=x64 --out=./release --asar=true --overwrite --version=1.4.0"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "async": "^1.3.0",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "iconv-lite": "^0.4.10",
    "jquery": "^2.1.4",
    "lodash": "^4.17.4",
    "mac-open": "^0.1.3",
    "mkdirp": "^0.5.1",
    "request": "^2.58.0",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.3.1",
    "@types/electron": "^1.4.31",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "^4.14.52",
    "@types/node": "^6.0.61",
    "angular-cli": "1.0.0-beta.26",
    "archiver": "^1.2.0",
    "codelyzer": "~2.0.0-beta.1",
    "concurrently": "^3.1.0",
    "electron": "^1.4.12",
    "electron-packager": "^8.0.0",
    "electron-prebuilt": "^1.4.13",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-cli": "^1.0.1",
    "karma-electron": "^5.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "~4.0.13",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wellwind/8ComicDownloaderElectron.git"
  },
  "keywords": [
    "comic",
    "8comic",
    "comicbus",
    "downloader"
  ],
  "author": "Wellwind",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wellwind/8ComicDownloaderElectron/issues"
  }
}
